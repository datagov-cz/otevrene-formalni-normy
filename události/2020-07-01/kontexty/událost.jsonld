{
	"@context": {
		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"události": "https://slovník.gov.cz/generický/události/pojem/",
		"cpev": "http://data.europa.eu/m8g/",
		"Umístění": "dcterms:Location",
		"Událost": {
			"@id": "schema:Event",
			"@context": {
				"@version": 1.1,
				"@import": "https://ofn.gov.cz/umístění/2020-07-01/kontexty/umístění.jsonld",	
				"dlouhý_popis": {
					"@id": "události:dlouhý-popis",
					"@container": "@language"
				},
				"vhodné_pro_děti": {
					"@id": "události:vhodné-pro-děti",
					"@type": "xsd:boolean"
				},
				"vhodné_pro_zvířata": {
					"@id": "události:vhodné-pro-zvířata",
					"@type": "xsd:boolean"
				},
				"registrace": {
					"@id": "cpev:eventRegistration",
					"@container": "@language"
				},
				"pořadatel": {
					"@id": "cpev:hasOrganiser",
					"@context": "https://ofn.gov.cz/lidé-a-osoby/2020-07-01/kontexty/člověk-či-osoba.jsonld"
				},
				"hlavní_sponzor": {
					"@id": "cpev:hasSponsor",
					"@context": "https://ofn.gov.cz/lidé-a-osoby/2020-07-01/kontexty/člověk-či-osoba.jsonld"
				},
				"sponzor": {
					"@id": "cpev:hasSponsor",
					"@context": "https://ofn.gov.cz/lidé-a-osoby/2020-07-01/kontexty/člověk-či-osoba.jsonld"
				},
				"mediální_partner": {
					"@id": "cpev:hasPromoter",
					"@context": "https://ofn.gov.cz/lidé-a-osoby/2020-07-01/kontexty/člověk-či-osoba.jsonld"
				},
				"záštita": {
					"@id": "události:má-záštitu",
					"@context": "https://ofn.gov.cz/lidé-a-osoby/2020-07-01/kontexty/člověk-či-osoba.jsonld"
				},
				"umístění": "@nest",
				"typ_události": {
					"@id": "dcterms:type",
					"@type": "@id"
				},
				"téma": {
					"@id": "události:má-téma",
					"@type": "@id"
				},
				"bezbariérovost": {
					"@id": "cpev:accessibility",
					"@context": "https://ofn.gov.cz/bezbariérovost/2020-07-01/kontexty/bezbariérovost.jsonld"
				},
				"doba_trvání": {
					"@id": "schema:openingHoursSpecification",
					"@context": "https://ofn.gov.cz/časová-specifikace/2020-07-01/kontexty/časová-specifikace.jsonld"
				},
				"vstupné": {
					"@id": "události:má-vstupné",
					"@context": "https://ofn.gov.cz/vstupné/2020-07-01/kontexty/vstupné.jsonld"
				},
				"kontakt": {
					"@id": "události:má-kontakt",
					"@context": "https://ofn.gov.cz/kontakty/2020-07-01/kontexty/kontakt.jsonld"
				},
				"dílčí_událost": {
					"@id": "schema:subEvent",
					"@context": "https://ofn.gov.cz/události/2020-07-01/kontexty/událost.jsonld"
				},
				"zaštiťující_událost": {
					"@id": "schema:superEvent",
					"@context": "https://ofn.gov.cz/události/2020-07-01/kontexty/událost.jsonld"
				}
			}
		}
	}
}