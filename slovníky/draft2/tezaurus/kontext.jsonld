{
  "@context": {
    "@version": 1.1,
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "dcterms": "http://purl.org/dc/terms/",
    "slovníky": "https://slovník.gov.cz/generický/datový-slovník-ofn-slovníků/pojem/",
    "čas": "https://slovník.gov.cz/generický/čas/pojem/",
    "iri": "@id",
    "typ": "@type",
    "název": {
      "@id": "skos:prefLabel",
      "@container": "@language"
    },
    "popis": {
      "@id": "dcterms:description",
      "@container": "@language"
    },
    "vytvořeno": {
      "@id": "slovníky:okamžik-vytvoření",
      "@type": "@id",
      "@context": {
        "Časový okamžik": {
          "@id": "čas:časový-okamžik",
          "@context": {
            "je_nespecifikovaný": {
              "@id": "čas:je-nespecifikovaný",
              "@type": "xsd:boolean"
            },
            "datum_a_čas": {
              "@id": "čas:datum-a-čas",
              "@type": "xsd:dateTimeStamp"
            },
            "datum": {
              "@id": "čas:datum",
              "@type": "xsd:date"
            }
          }
        }
      }
    },
    "aktualizováno": {
      "@id": "slovníky:okamžik-poslední-změny",
      "@type": "@id",
      "@context": {
        "Časový okamžik": {
          "@id": "čas:časový-okamžik",
          "@context": {
            "je_nespecifikovaný": {
              "@id": "čas:je-nespecifikovaný",
              "@type": "xsd:boolean"
            },
            "datum_a_čas": {
              "@id": "čas:datum-a-čas",
              "@type": "xsd:dateTimeStamp"
            },
            "datum": {
              "@id": "čas:datum",
              "@type": "xsd:date"
            }
          }
        }
      }
    },
    "pojmy": {
      "@reverse": "skos:inScheme",
      "@container": "@set",
      "@context": {
        "název": {
          "@id": "skos:prefLabel",
          "@container": "@language"
        },
        "alternativní-název": {
          "@id": "skos:altLabel",
          "@container": "@language"
        },
        "popis": {
          "@id": "dcterms:description",
          "@container": "@language"
        },
        "definice": {
          "@id": "skos:definition",
          "@container": "@language"
        },
        "definující-ustanovení-právního-předpisu": {
          "@id": "dcterms:conformsTo",
          "@type": "@id",
          "@container": "@set"
        },
        "související-ustanovení-právního-předpisu": {
          "@id": "dcterms:relation",
          "@type": "@id",
          "@container": "@set"
        },
        "ekvivalentní-pojem": {
          "@id": "skos:exactMatch",
          "@type": "@id",
          "@container": "@set"
        },
        "nadřazený-pojem": {
          "@id": "skos:broader",
          "@type": "@id",
          "@container": "@set"
        },
        "Koncept": {
          "@id": "skos:Concept",
          "@context": {}
        },
        "Pojem": {
          "@id": "slovníky:pojem",
          "@context": {}
        }
      }
    },
    "Tezaurus": {
      "@id": "skos:ConceptScheme",
      "@context": {}
    },
    "Slovník": {
      "@id": "slovníky:slovník",
      "@context": {}
    }
  }
}