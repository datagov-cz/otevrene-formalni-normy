{
  "@context": {
    "@version": 1.1,
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "dcterms": "http://purl.org/dc/terms/",
    "čas": "https://slovník.gov.cz/generický/čas/pojem/",
    "slovníky": "https://slovník.gov.cz/generický/datový-slovník-ofn-slovníků/pojem/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "iri": "@id",
    "typ": "@type",
    "Slovník": {
      "@id": "slovníky:slovník",
      "@context": {
        "název": {
          "@id": "skos:prefLabel",
          "@container": "@language"
        },
        "popis": {
          "@id": "dcterms:description",
          "@container": "@language"
        },
        "vytvořeno": {
          "@id": "slovníky:okamžik-vytvoření",
          "@type": "@id",
          "@context": {
            "Časový okamžik": {
              "@id": "čas:časový-okamžik",
              "@context": {
                "je_nespecifikovaný": {
                  "@id": "čas:je-nespecifikovaný",
                  "@type": "xsd:boolean"
                },
                "datum_a_čas": {
                  "@id": "čas:datum-a-čas",
                  "@type": "xsd:dateTime"
                },
                "datum": {
                  "@id": "čas:datum",
                  "@type": "xsd:date"
                }
              }
            }
          }
        },
        "aktualizováno": {
          "@id": "slovníky:okamžik-poslední-změny",
          "@type": "@id",
          "@context": {
            "Časový okamžik": {
              "@id": "čas:časový-okamžik",
              "@context": {
                "je_nespecifikovaný": {
                  "@id": "čas:je-nespecifikovaný",
                  "@type": "xsd:boolean"
                },
                "datum_a_čas": {
                  "@id": "čas:datum-a-čas",
                  "@type": "xsd:dateTime"
                },
                "datum": {
                  "@id": "čas:datum",
                  "@type": "xsd:date"
                }
              }
            }
          }
        }
      }
    }
  }
}