{
	"@context": {
		"@version": 1.1,
		"owl": "http://www.w3.org/2002/07/owl#",
		"zsgov": "https://slovník.gov.cz/základní/pojem/mixin-rolí",
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"Seznam pojmů": {
			"@id": "skos:ConceptScheme",
			"@context": {
				"@version": 1.1,
				"název": {
					"@id": "skos:prefLabel",
					"@container": "@language"
				},
				"definice": {
					"@id": "skos:definition",
					"@container": "@language"
				},
				"pojmy": {
					"@reverse": "skos:inScheme",
					"@context": {
						"Pojem": {
							"@id": "skos:Concept",
							"@context": {
								"název": {
									"@id": "skos:prefLabel",
									"@container": "@language"
								},
								"definice": {
									"@id": "skos:definition",
									"@container": "@language"
								}		
							}
						},
						"Třída": {
							"@id": "owl:Class",
							"@context": {
								"nadřazená-třída": {
									"@id": "rdfs:subClassOf",
									"@type": "@id"
								}
							}
						},
						"Vlastnost": {
							"@id": "owl:DatatypeProperty",
							"@context": {
								"definiční-obor": {
									"@id": "rdfs:domain",
									"@type": "@id"
								},
								"datový-typ": {
									"@id": "rdfs:range",
									"@type": "@id"
								}
							}
						},
						"Vztah": {
							"@id": "owl:ObjectProperty",
							"@context": {
								"definiční-obor": {
									"@id": "rdfs:domain",
									"@type": "@id"
								},
								"obor-hodnot": {
									"@id": "rdfs:range",
									"@type": "@id"
								}
							}
						},
						"Osoba": "https://slovník.gov.cz/veřejný-sektor/pojem/subjekt-práva",
						"Věc": "https://slovník.gov.cz/veřejný-sektor/pojem/objekt-práva",
						"Role": "zsgov:mixin-rolí",
						"Fáze": "zsgov:mixin-fází",
						"Činnost": "https://slovník.gov.cz/legislativní/sbírka/111/2009/pojem/činnost",
						"Událost": "zsgov:událost"
					}
				}
			}
		}
    }
}