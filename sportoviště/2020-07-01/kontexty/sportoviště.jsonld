{
	"@context": {
		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"sportoviště": "https://slovník.gov.cz/generický/sportoviště/pojem/",
		"veřejná-místa": "https://slovník.gov.cz/generický/veřejná-místa/pojem/",
		"místa": "https://slovník.gov.cz/generický/místa/pojem/",
		"Umístění": "dcterms:Location",
		"Sportoviště": {
			"@id": "schema:SportsActivityLocation",
			"@context": {
				"@version": 1.1,
				"@import": "https://ofn.gov.cz/umístění/2020-07-01/kontexty/umístění.jsonld",	
				"podmínky_užívání": {
					"@id": "sportoviště:podmínky-užívání",
					"@container": "@language"
				},
				"provozní_řád": {
					"@id": "sportoviště:provozní-řád",
					"@container": "@language"
				},
				"provozovatel": {
					"@id": "veřejná-místa:má-provozovatele",
					"@context": "https://ofn.gov.cz/lidé-a-osoby/2020-07-01/kontexty/člověk-či-osoba.jsonld"
				},
				"vlastník": {
					"@id": "veřejná-místa:má-vlastníka",
					"@context": "https://ofn.gov.cz/lidé-a-osoby/2020-07-01/kontexty/člověk-či-osoba.jsonld"
				},
				"umístění": "@nest",
				"kontakt": {
					"@id": "veřejná-místa:má-kontakt",
					"@context": "https://ofn.gov.cz/kontakty/2020-07-01/kontexty/kontakt.jsonld"
				},
				"bezbariérovost": {
					"@id": "veřejná-místa:má-bezbariérový-přístup",
					"@context": "https://ofn.gov.cz/bezbariérovost/2020-07-01/kontexty/bezbariérovost.jsonld"
				},
				"vstupné": {
					"@id": "veřejná-místa:má-vstupné",
					"@context": "https://ofn.gov.cz/vstupné/2020-07-01/kontexty/vstupné.jsonld"
				},
				"otevírací_doba": {
					"@id": "gr:hasOpeningHoursSpecification",
					"@context": "https://ofn.gov.cz/časová-specifikace/2020-07-01/kontexty/časová-specifikace.jsonld"
				},
                "doplňková_služba": {
					"@id": "sportoviště:má-doplňkovou-službu",
					"@context": {
						"Služba": {
							"@id": "sportoviště:služba"
						}
					}
                },
                "typ_sportoviště": {
					"@id": "sportoviště:má-typ-sportoviště",
					"@type": "@id"
				},
				"sport": {
					"@id": "sportoviště:má-provozovaný-sport",
					"@type": "@id"
				}
			}
		}
	}
}