{
	"@context": {
		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"osoby": "https://slovník.gov.cz/generický/právnické-osoby/pojem/",
		"rov": "http://www.w3.org/ns/regorg#",
		"Osoba": {
			"@id": "rov:RegisteredOrganization",
			"@context": {
				"název": {
					"@id": "rov:legalName",
					"@container": "@language"
				},
				"ičo": {
					"@id": "skos:notation",
					"@type": "xsd:string"
				}
			}
		},
		"Zahraniční osoba": {
			"@id": "schema:Organization",
			"@context": {
				"název": {
					"@id": "schema:legalName",
					"@container": "@language"
				},
				"stát_registrace": {
					"@id": "osoby:stát-registrace",
					"@type": "@id"
				}
			}
		},
		"Člověk": {
			"@id": "schema:Person",
			"@context": {
				"jméno": {
					"@id": "schema:givenName",
					"@container": "@language"
				},
				"příjmení": {
					"@id": "schema:familyName",
					"@container": "@language"
				},
				"tituly_před": {
					"@id": "schema:honorificPrefix",
					"@container": "@language"
				},
				"tituly_za": {
					"@id": "schema:honorificSuffix",
					"@container": "@language"
				},
				"pohlaví": {
					"@id": "schema:gender",
					"@type": "@id"
				},
				"text": {
					"@id": "schema:name",
					"@container": "@language"
				}
			}
		}
	}
}