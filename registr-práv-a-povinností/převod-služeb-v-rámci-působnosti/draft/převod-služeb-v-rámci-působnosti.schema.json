{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://ofn.gov.cz/registr-práv-a-povinností/převod-službaů-v-rámci-působnosti/draft/převod-službaů-v-rámci-působnosti.schema.json",
  "type": "object",
  "required": [
    "položky"
  ],
  "properties": {
    "položky": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "služba": {
            "type": "string",
            "examples": [
              "služba/S61"
            ],
            "pattern": "^služba/S[0-9]+$"
          },
          "z-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A3787/00572161"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+(\\.[0-9]+)?$"
          },
          "do-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A3787/00253316"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+(\\.[0-9]+)?$"
          },
          "přebírající-pracoviště": {
            "type": "array",
            "examples": [
              "pracoviště/41894"
            ],
            "items": {
              "type": "string",
              "pattern": "^pracoviště/[0-9]+$"
            }
          },
          "od": {
            "type": "string",
            "examples": [
              "2018-09-08"
            ],
            "format": "date"
          },
          "do": {
            "type": "string",
            "examples": [
              "2020-01-02"
            ],
            "format": "date"
          },
          "dokument-převodu-agend": {
            "type": "string",
            "examples": [
              "dokument-převodu-agend/RPA1546"
            ],
            "pattern": "^dokument-převodu-agend/.+$"
          }
        }
      }
    }
  }
}