{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://data.gov.cz/otevřené-formální-normy/registr-práv-a-povinností/kategorie-soukromoprávních-uživatelů-údajů.schema.json",
  "required": [
    "položky"
  ],
  "type": "object",
  "properties": {
    "položky": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "default": "kategorie-spuú",
            "pattern": "^kategorie-spuú$"
          },
          "id": {
            "type": "string",
            "examples": [
              "kategorie-spuú/KU4"
            ],
            "pattern": "^kategorie-spuú/KU[0-9]+$"
          },
          "název": {
            "type": "object",
            "examples": [
              {"cs" : "Pojišťovny"}
            ],
            "properties": {
              "cs": {
                "type": "string"
              }
            }
          },
          "identifikátor": {
            "type": "string",
            "examples": [
              "KU4"
            ],
            "pattern": "^KU[0-9]+$"
          },
          "ustanovení": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "default": "ustanovení-právního-předpisu",
                  "pattern": "^ustanovení-právního-předpisu$"
                },
                "označení": {
                  "type": "string",
                  "examples": [
                    "§ 129 zákona č. 277/2009 Sb. - Zákon o pojišťovnictví"
                  ]
                }
              }
            }
          },
          "ohlašovatel": {
            "type": "string",
            "examples": [
              "orgán-veřejné-moci/48136450"
            ],
            "pattern": "^orgán-veřejné-moci/[0-9]{8}(\\.[0-9]+)?$"
          }
        }
      }
    }
  }
}