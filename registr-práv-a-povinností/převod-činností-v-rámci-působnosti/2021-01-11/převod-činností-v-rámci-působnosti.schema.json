{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://ofn.gov.cz/registr-práv-a-povinností/převod-činností-v-rámci-působnosti/2021-01-11/převod-činností-v-rámci-působnosti.schema.json",
  "type": "object",
  "required": [
    "položky"
  ],
  "properties": {
    "položky": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "činnost": {
            "type": "string",
            "examples": [
              "činnost/A1185/CR8359"
            ],
            "pattern": "^činnost/A[0-9]+/CR[0-9]+$"
          },
          "z-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A1185/00562424"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+(\\.[0-9]+)?$"
          },
          "do-působnosti": {
            "type": "string",
            "examples": [
              "působnost/A1185/00297577"
            ],
            "pattern": "^působnost/A[0-9]+/[0-9]+(\\.[0-9]+)?$"
          },
          "od": {
            "type": "string",
            "examples": [
              "2019-05-20"
            ],
            "format": "date"
          },
          "do": {
            "type": "string",
            "examples": [
              "2019-05-20"
            ],
            "format": "date"
          },
          "dokument-převodu-agend": {
            "type": "string",
            "examples": [
              "dokument-převodu-agend/00296333"
            ],
            "pattern": "^dokument-převodu-agend/RPA261+$"
          }
        }
      }
    }
  }
}