{
	"@context": {
		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.ssp.jsonld",
		"@propagate": true,
		"vivo": "http://vivoweb.org/ontology/core#",
		"Výzkumník": {
			"@id": "schema:Person",
			"@context": {
				"jméno": {
					"@id": "schema:givenName",
					"@container": "@language"
				},
				"příjmení": {
					"@id": "schema:familyName",
					"@container": "@language"
				},
				"tituly_před": {
					"@id": "schema:honorificPrefix",
					"@container": "@language"
				},
				"tituly_za": {
					"@id": "schema:honorificSuffix",
					"@container": "@language"
				},
				"pohlaví": {
					"@id": "schema:gender",
					"@type": "@id"
				},
				"text": {
					"@id": "schema:name",
					"@container": "@language"
				},
				"orcid": {
					"@id": "vivo:orcidId",
					"@type": "@id"
				},
				"researcherid": {
					"@id": "vivo:researcherId",
					"@type": "xsd:string"
				},
				"scopus_author_id": {
					"@id": "vivo:scopusId",
					"@type": "xsd:string"
				}
			},
			"Výzkumné pracoviště": {
				"@id": "nedefinováno:výzkumné-pracoviště",
				"@context": [
					"https://pod-test.mvcr.gov.cz/otevřené-formální-normy/práce/draft/kontexty/práce.ssp.jsonld",
					{
						"orjk": {
							"@id": "nedefinováno:orjk",
							"@type": "xsd:string"
						}
					}
				]
			}
		}
	}
}