{
	"@context": {
		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"vivo": "http://vivoweb.org/ontology/core#",
		"věda": "https://slovník.gov.cz/generický/věda-a-výzkum/pojem/",
		"dbpedia": "http://dbpedia.org/ontology/",
		"Výzkumník": {
			"@id": "dbo:Scientist",
			"@context": {
				"jméno": {
					"@id": "schema:givenName",
					"@container": "@language"
				},
				"příjmení": {
					"@id": "schema:familyName",
					"@container": "@language"
				},
				"tituly_před": {
					"@id": "schema:honorificPrefix",
					"@container": "@language"
				},
				"tituly_za": {
					"@id": "schema:honorificSuffix",
					"@container": "@language"
				},
				"pohlaví": {
					"@id": "schema:gender",
					"@type": "@id"
				},
				"text": {
					"@id": "schema:name",
					"@container": "@language"
				},
				"orcid": {
					"@id": "vivo:orcidId",
					"@type": "@id"
				},
				"researcherid": {
					"@id": "vivo:researcherId",
					"@type": "xsd:string"
				},
				"scopus_author_id": {
					"@id": "vivo:scopusId",
					"@type": "xsd:string"
				}
			}
		},
		"Výzkumné pracoviště": {
			"@id": "vivo:ResearchOrganization",
			"@context": [
				"https://ofn.gov.cz/práce/2021-09-23/kontexty/práce.jsonld",
				{
					"orjk": {
						"@id": "věda:orjk",
						"@type": "xsd:string"
					}
				}
			]
		}
	}
}