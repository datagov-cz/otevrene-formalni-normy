		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"turistické-cíle": "https://slovník.gov.cz/generický/turistické-cíle/pojem/",
