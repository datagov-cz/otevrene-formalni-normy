{
	"@context": {
		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"čas": "https://slovník.gov.cz/generický/čas/pojem/",
		"Časová specifikace": {
			"@id": "schema:Schedule",
			"@context": {
				"počet_opakování": {
					"@id": "schema:repeatCount",
					"@type": "xsd:integer"
				},
				"časový_okamžik": {
					"@id": "čas:má-časový-okamžik",
					"@context": "https://ofn.gov.cz/základní-datové-typy/2020-07-01/kontexty/časový-okamžik.jsonld"
				},
				"časový_interval": {
					"@id": "čas:má-časový-interval",
					"@context": "https://ofn.gov.cz/základní-datové-typy/2020-07-01/kontexty/časový-interval.jsonld"
				},
				"jiná_časová_specifikace": {
					"@id": "čas:má-jinou-časovou-specifikaci",
					"@type": "@id"
				},
				"den_v_týdnu": {
					"@id": "čas:má-den-v-týdnu",
					"@type": "@id"
				},
				"časové_období": {
					"@id": "čas:má-časové-období",
					"@type": "@id"
				},
				"frekvence": {
					"@id": "schema:repeatFrequency",
					"@type": "@id"
				},
				"časová_doba": {
					"@id": "čas:má-časovou-dobu",
					"@context": {
						"Časová doba": {
							"@id": "čas:časová-doba",
							"@context": {
								"čas": {
									"@id": "https://slovník.gov.cz/generický/čas/pojem/čas",
									"@type": "xsd:time"
								},
								"od": {
									"@id": "https://slovník.gov.cz/generický/čas/pojem/od",
									"@type": "xsd:time"
								},
								"do": {
									"@id": "https://slovník.gov.cz/generický/čas/pojem/do",
									"@type": "xsd:time"
								}
							}
						}
					}
				},
				"specifická_frekvence": {
					"@id": "čas:má-specifickou-frekvenci",
					"@context": {
						"Specifická frekvence": {
							"@id": "čas:specifická-frekvence",
							"@context": {
								"minuta": {
									"@id": "čas:minuta",
									"@type": "xsd:integer"
								},
								"hodina": {
									"@id": "čas:hodina",
									"@type": "xsd:integer"
								},
								"den_v_měsíci": {
									"@id": "čas:den-v-měsíci",
									"@type": "xsd:integer"
								},
								"týden_v_měsíci": {
									"@id": "čas:týden-v-měsíci",
									"@type": "xsd:integer"
								},
								"týden_v_roce": {
									"@id": "čas:týden-v-roce",
									"@type": "xsd:integer"
								},
								"rok_v_desetiletí": {
									"@id": "čas:rok-v-desetiletí",
									"@type": "xsd:integer"
								},
								"rok_ve_století": {
									"@id": "čas:rok-ve-století",
									"@type": "xsd:integer"
								}
							}
						}
					}
				},
				"výjimka": {
					"@id": "čas:výjimka",
					"@context": "https://ofn.gov.cz/časová-specifikace/2020-07-01/kontexty/časová-specifikace.jsonld"
				},
				"časová_platnost": {
					"@id": "čas:časová-platnost",
					"@context": "https://ofn.gov.cz/časová-specifikace/2020-07-01/kontexty/časová-specifikace.jsonld"
				}
			}
		}
	}
}