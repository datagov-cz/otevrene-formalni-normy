{
	"@context": {
		"@version": 1.1,
		"locn": "http://www.w3.org/ns/locn#",
		"dcterms": "http://purl.org/dc/terms/",
		"název_umístění": {
			"@id": "locn:geographicName",
			"@container": "@language"
		},
		"popis_umístění": {
			"@id": "dcterms:description",
			"@container": "@language"
		},
		"geometrie": {
			"@id": "locn:geometry",
			"@context": "https://geojson.org/geojson-ld/geojson-context.jsonld"
		},
		"adresa": {
			"@id": "locn:address",
			"@context": "https://ofn.gov.cz/adresy/2020-07-01/kontexty/adresa.jsonld"
		},
		"budova": {
			"@id": "locn:locatorName",
			"@container": "@language"
		},
		"patro": {
			"@id": "locn:locatorName",
			"@container": "@language"
		},
		"místnost": {
			"@id": "locn:locatorName",
			"@container": "@language"
		},
		"zahraniční_adresa_text": {
			"@id": "locn:fullAddress",
			"@container": "@language"
		}
	}
}