{
  "@context": {
    "@version": 1.1,
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "vcard": "http://www.w3.org/2006/vcard/ns#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "iri": "@id",
    "typ": "@type",
    "Katalog": {
      "@id": "dcat:Catalog",
      "@context": {
        "název": {
          "@id": "dct:title",
          "@container": "@language"
        },
        "popis": {
          "@id": "dct:description",
          "@container": "@language"
        },
        "kontaktní_bod": {
          "@id": "dcat:contactPoint",
          "@type": "@id",
          "@context": {
            "Organizace": {
              "@id": "vcard:Organization",
              "@context": {
                "jméno": {
                  "@id": "vcard:fn",
                  "@container": "@language"
                },
                "e-mail": {
                  "@id": "vcard:hasEmail",
                  "@type": "@id"
                }
              }
            }
          }
        },
        "domovská_stránka": {
          "@id": "foaf:homepage",
          "@type": "@id"
        },
        "poskytovatel": {
          "@id": "dct:publisher",
          "@type": "@id"
        },
        "datová_sada": {
          "@id": "dcat:dataset",
          "@type": "@id",
          "@container": "@set"
        }
      }
    }
  }
}