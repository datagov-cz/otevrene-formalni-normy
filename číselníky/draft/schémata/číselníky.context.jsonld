{
  "@context": {
    "položky": "@graph",
    "typ": "@type",
    "id": "@id",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "dct": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "číselník": "skos:ConceptScheme",
    "položka": "skos:Concept",
    "název-číselníku": {
      "@id": "skos:prefLabel",
      "@container": "@language"
    },
    "popis-číselníku": {
      "@id": "skos:note",
      "@container": "@language"
    },
    "platnost-číselníku": {
      "@id": "dct:temporal"
    },
    "položky": {
      "@reverse": "skos:inScheme",
      "@container": "@set"
    },
    "kód": {
      "@id": "skos:notation",
      "@type": "xsd:string"
    },
    "název": {
      "@id": "skos:prefLabel",
      "@container": "@language"
    },
    "alt-název": {
      "@id": "skos:altLabel",
      "@container": "@language"
    },
    "definice": {
      "@id": "skos:definition",
      "@container": "@language"
    },
    "popis": {
      "@id": "skos:note",
      "@container": "@language"
    },
    "platnost": {
      "@id": "dct:temporal",
      "@type": "@id"
    },
    "datum_začátku": {
      "@id": "dcat:startDate",
      "@type": "xsd:date"
    },
    "datum_konce": {
      "@id": "dcat:endDate",
      "@type": "xsd:date"
    },
    "přesná-shoda": {
      "@id": "skos:exactMatch",
      "@type": "@id"
    },
    "blízká-shoda": {
      "@id": "skos:closeMatch",
      "@type": "@id"
    },
    "širší-souvislost": {
      "@id": "skos:broadMatch",
      "@type": "@id"
    },
    "užší-souvislost": {
      "@id": "skos:narrowMatch",
      "@type": "@id"
    },
    "souvislost": {
      "@id": "skos:narrowMatch",
      "@type": "@id"
    }
  }
}