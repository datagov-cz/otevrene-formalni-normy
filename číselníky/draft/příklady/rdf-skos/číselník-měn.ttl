pol:064 a skos:Concept;
  skos:notation "064";
  skos:prefLabel "bhútánský ngultrum"@cs, "bhutanese ngultrum"@en;
  skos:altLabel "ngultrum"@cs, "ngultrum"@en, "BTN"@cs, "BTN"@en;
  skos:definition "1 ngultrum=100 chhetrum"@cs, "1 ngultrum=100 chhetrum"@en;
  skos:note "Bhútánský ngultrum je měnou asijského království Bhútán. Jeho ISO 4217 kód je BTN."@cs,
    "The ngultrum is the currency of the Kingdom of Bhutan. Its ISO 4217 code is BTN."@en;
  skos:inScheme cis:měny .

pol:068 a skos:Concept;
  skos:notation "068";
  skos:prefLabel "bolivijské boliviano"@cs, "bolivian boliviano"@en;
  skos:altLabel "boliviano"@cs, "boliviano"@en, "BOB"@cs, "BOB"@en;
  skos:definition "1 boliviano=100 centavo"@cs, "1 boliviano=100 centavo"@en;
  skos:note "Boliviano (množné číslo bolivianos) je zákonným platidlem jihoamerického státu Bolívie. Jeho ISO 4217 kód je BOB."@cs,
    "The boliviano is the currency of the south american state Bolivia. Its ISO 4217 code is BOB."@en;
  skos:inScheme cis:měny .

