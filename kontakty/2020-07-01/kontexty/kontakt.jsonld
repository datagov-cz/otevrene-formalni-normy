{
	"@context": {
		"@version": 1.1,
		"@import": "https://ofn.gov.cz/věc/2020-07-01/kontexty/věc.jsonld",
		"@propagate": true,
		"kontakty": "https://slovník.gov.cz/generický/kontakty/pojem/",
		"Umístění": "dcterms:Location",
		"Kontakt": {
			"@id": "vcard:VCard",
			"@context": {
				"@version": 1.1,
				"@import": "https://ofn.gov.cz/umístění/2020-07-01/kontexty/umístění.jsonld",	
				"druh": {
					"@id": "kontakty:druh",
					"@container": "@language"
				},				
				"email": {
					"@id": "vcard:hasEmail",
					"@type": "@id"
				},
				"facebook": {
					"@id": "kontakty:facebook",
					"@type": "xsd:string"
				},
				"idds": {
					"@id": "kontakty:má-datovou-schránku",
					"@type": "xsd:string"
				},
				"instagram": {
					"@id": "kontakty:instagram",
					"@type": "xsd:string"
				},
				"mobil": {
					"@id": "vcard:hasTelephone",
					"@type": "@id"
				},
				"telefon": {
					"@id": "vcard:hasTelephone",
					"@type": "@id"
				},
				"twitter": {
					"@id": "kontakty:twitter",
					"@type": "xsd:string"
				},
				"url": {
					"@id": "vcard:hasURL",
					"@type": "xsd:anyURI"
				},
				"umístění": "@nest"
			}
		}
	}
}