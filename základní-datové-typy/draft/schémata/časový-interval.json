{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://ofn.gov.cz/základní-datové-typy/draft/schémata/časový-interval.json",
	"title": "Časový interval",
	"type": "object",
	"description": "Časový interval - začátek a konec",
	"anyOf": [{
			"required": ["datum_začátku"],
			"properties": {
				"datum_začátku": {
					"$id": "#/anyOf/properties/datum_začátku",
					"type": "string",
					"format": "date",
					"title": "Datum začátku intervalu.",
					"examples": [
						"2019-01-01+01:00"
					]
				}
			}
		},
		{
			"required": ["datum_konce"],
			"properties": {
				"datum_konce": {
					"$id": "#/anyOf/properties/datum_konce",
					"type": "string",
					"format": "date",
					"title": "Datum konce intervalu.",
					"examples": [
						"2020-01-01"
					]
				}
			}
		},
		{
			"required": ["čas_začátku"],
			"properties": {
				"čas_začátku": {
					"$id": "#/anyOf/properties/čas_začátku",
					"type": "string",
					"format": "time",
					"title": "Čas začátku intervalu.",
					"examples": [
						"09:30:00"
					]
				}
			}
		}, {

			"required": ["čas_konce"],
			"properties": {
				"čas_konce": {
					"$id": "#/anyOf/čas_konce",
					"type": "string",
					"format": "time",
					"title": "Čas konce intervalu.",
					"examples": [
						"09:30:10.5+01:00"
					]
				}
			}
		},
		{

			"required": ["datum_a_čas_začátku"],
			"properties": {
				"datum_a_čas_začátku": {
					"$id": "#/anyOf/properties/datum_a_čas_začátku",
					"type": "string",
					"format": "date-time",
					"title": "Datum a čas začátku intervalu.",
					"examples": [
						"2019-01-01T09:30:00"
					]
				}
			}
		},
		{

			"required": ["datum_a_čas_konce"],
			"properties": {
				"datum_a_čas_konce": {
					"$id": "#/anyOf/properties/datum_a_čas_konce",
					"type": "string",
					"format": "date-time",
					"title": "Datum a čas konce intervalu.",
					"examples": [
						"2019-01-01T09:30:10.5+01:00"
					]
				}
			}
		}
	]
}