{
	"@context": {
        "@version": 1.1,
		"typ": "@type",
		"iri": "@id",
        "time": "http://www.w3.org/2006/time#",
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"časy": "https://slovník.gov.cz/generický/čas/pojem/",
		"Časový okamžik": {
			"@id": "time:Instant",
			"@context": {
				"datum": {
					"@id": "time:inXSDDate",
					"@type": "xsd:date"
				},
				"datum_a_čas": {
					"@id": "time:inXSDDateTimeStamp",
					"@type": "xsd:dateTimeStamp"
				},
				"nespecifikovaný": {
					"@id": "časy:je-nespecifikovaný",
					"@type": "xsd:boolean"
				}
			}
		}
	}
}