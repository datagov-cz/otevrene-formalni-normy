{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "https://ofn.gov.cz/základní-datové-typy/2020-07-01/schémata/množství.json",
	"type": "object",
	"title": "Množství",
	"required": ["typ", "hodnota", "jednotka"],
	"properties": {
		"typ": {
			"oneOf": [{
				"const": "Množství"
				},{
				"type": "array",
				"contains": {
					"const": "Množství"
				},
				"items": {
					"type": "string"
				}
			}]
		},
		"hodnota": {
			"type": "number",
			"title": "Hodnota",
			"examples": [
				1000.1
			]
		},
		"jednotka": {
			"type": "string",
			"title": "Jednotka",
			"examples": [
				"KGM"
			]
		}
	}
}