{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "https://ofn.gov.cz/základní-datové-typy/2020-07-01/schémata/text.json",
	"name": "Text",
	"type": "object",
	"description": "Vícejazyčný text",
	"anyOf": [{
			"required": ["cs"],
			"properties": {
				"cs": {
					"type": "string",
					"title": "Text česky",
					"description": "Text česky",
					"examples": [
						"Text česky"
					]
				}
			}
		},
		{
			"required": ["en"],
			"properties": {
				"en": {
					"type": "string",
					"title": "Text anglicky",
					"description": "Text anglicky",
					"examples": [
						"Text in English"
					]
				}
			}
		}
	],
	"additionalProperties": {
		"type": "string"
	}
}